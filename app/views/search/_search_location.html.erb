<%= form_tag(nearby_spots_path, method: "get") do %>
  <%= text_field_tag(:q, nil, placeholder: "Los Angeles, CA or 90001", :required => true) %>
  <%= label_tag(:radius, "Radius in miles") %>
  <%= text_field_tag(:radius, "5") %>
  <%= submit_tag("Find Parking!", class: "btn btn-large" ) %>
<% end %>
